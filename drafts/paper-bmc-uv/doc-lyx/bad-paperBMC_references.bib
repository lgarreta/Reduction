issn = {1664-462X},
journal = {Frontiers in Plant Science},
keywords = {Association mapping,GWAS,GWAS Paper,GWAS Potato,MultiGWAS Paper,Paper-MultiGWAS,Potato,SNP,Tassel Cases,candidate genes,late blight,quantitative disease resistance},
mendeley-tags = {GWAS,GWAS Paper,GWAS Potato,MultiGWAS Paper,Paper-MultiGWAS,Tassel Cases},
