keywords = {Algorithms Paper,Artificial duplicates,Clustering,Fast Clustering,Hobohm Algorithm,Hobohm References,Metagenomics,Metagenomics Analysis,Metagenomics Clustering,Metagenomics Software,Motifs Clustering,Next-generation sequencing,OTU,Protein families,Uclust},
