issn = {0031949X},
journal = {Phytopathology},
keywords = {Diploidized Models,GWAS,GWAS Diploidization,GWAS Potato,MultiGWAS Paper,Paper-MultiGWAS,Potato},
mendeley-tags = {Diploidized Models,GWAS,GWAS Diploidization,GWAS Potato,MultiGWAS Paper,Paper-MultiGWAS,Potato},
number = {6},
pages = {624--633},
title = {{Phenotypic stability and genome-wide association study of late blight resistance in potato genotypes adapted to the tropical highlands}},
